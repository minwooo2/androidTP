<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.teamProject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ImageGame">
        <activity android:name="com.example.teamProject.Result3"
            android:exported="false"/>
        <activity android:name="com.example.teamProject.Result2"
            android:exported="false"/>
        <activity android:name="com.example.teamProject.Result1"
            android:exported="false"/>
        <activity android:name="com.example.teamProject.Start"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.teamProject.Select"
            android:exported="false"/>
        <activity android:name="com.example.teamProject.Game1"
            android:exported="false"/>
        <activity android:name="com.example.teamProject.Game2"
            android:exported="false"/>
        <activity
            android:name="com.example.teamProject.Game3"
            android:exported="false"/>

    </application>

</manifest>